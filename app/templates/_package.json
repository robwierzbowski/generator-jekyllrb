{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "description": "Yeoman / Grunt workflow for <%= appname %>",
  "dependencies": {},
  "devDependencies": {
    "grunt": "~0.4.1",<% if (autoPre) { %>
    "grunt-autoprefixer": "~0.2.0",<% } %>
    "grunt-contrib-clean": "~0.5.0",<% if (jsPre === 'coffeescript') { %>
    "grunt-contrib-coffee": "~0.7.0",<% } %><% if (cssPre === 'compass') { %>
    "grunt-contrib-compass": "~0.5.0",<% } %>
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-csslint": "~0.1.2",
    "grunt-contrib-cssmin": "~0.6.0",
    "grunt-contrib-htmlmin": "~0.1.3",
    "grunt-contrib-imagemin": "~0.2.0",
    "grunt-contrib-jshint": "~0.6.3",<% if (cssPre === 'stylus') { %>
    "grunt-contrib-stylus": "~0.8.0",<% } %>
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-watch": "~0.5.2",
    "grunt-concurrent": "~0.3.0",
    "grunt-csscss": "~0.5.0",
    "grunt-jekyll": "git://github.com/dannygarcia/grunt-jekyll.git#aa03074ee0f5b69dc9d7a0e6ced6bb8cd923add6",
    "grunt-rev": "~0.1.0",<% if (cssPre === 'sass') { %>
    "grunt-contrib-sass": "~0.5.0",<% } %>
    "grunt-svgmin": "~0.2.0",
    "grunt-usemin": "~0.1.12",
    "load-grunt-tasks": "~0.2.0",
    "time-grunt": "~0.1.1",
    "jshint-stylish": "~0.1.3"
  },
  "engines": {
    "node": ">=0.8.0"
  }
