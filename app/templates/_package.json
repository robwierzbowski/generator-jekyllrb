{
  "name": "<%= _.slugify(appname) %>",
  "private": true,
  "version": "0.0.0",
  "description": "Yeoman workflow for <%= appname %>",
  "dependencies": {},
  "devDependencies": {
    "grunt": "~0.4.5",<% if (autoPre) { %>
    "grunt-autoprefixer": "~1.0.1",<% } %><% if (deploy) { %>
    "grunt-build-control": "~0.1.3",<% } %>
    "grunt-browser-sync": "^2.1.3",
    "grunt-concurrent": "~0.5.0",
    "grunt-contrib-clean": "~0.6.0",<% if (jsPre === 'coffeescript') { %>
    "grunt-coffeelint": "~0.0.13",
    "grunt-contrib-coffee": "~0.11.1",<% } %><% if (cssPre === 'compass') { %>
    "grunt-contrib-compass": "~1.0.0",<% } %>
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-csslint": "~0.4.0",
    "grunt-contrib-cssmin": "~0.10.0",
    "grunt-contrib-htmlmin": "~0.3.0",
    "grunt-contrib-imagemin": "~0.8.1",
    "grunt-contrib-jshint": "~0.10.0",<% if (cssPre === 'sass') { %>
    "grunt-contrib-sass": "~0.8.1",<% } %>
    "grunt-contrib-uglify": "~0.5.1",
    "grunt-contrib-watch": "0.6.1",
    "grunt-filerev": "~0.2.1",
    "grunt-jekyll": "~0.4.2",
    "grunt-svgmin": "~0.4.0",
    "grunt-usemin": "~2.4.0",
    "jshint-stylish": "~0.4.0",
    "load-grunt-tasks": "~0.6.0",
    "time-grunt": "~0.4.0"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
