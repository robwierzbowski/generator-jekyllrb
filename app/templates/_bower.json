{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "dependencies": {<%
    var depn = [];
    if (h5bpCss) {
      depn.push('\n    "normalize-css": "~2.1.3"');
    }
    if (h5bpJs) {
      depn.push('\n    "jquery": "~1.10.2"');
      depn.push('\n    "modernizr": "~2.6.2"');
    }
    print(depn.join(','));
  %>
  }
}
